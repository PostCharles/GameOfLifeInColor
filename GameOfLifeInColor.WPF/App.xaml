<Application x:Class="GameOfLifeInColor.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Support.xaml" />
                <ResourceDictionary Source="/Resources/ValueConverters.xaml"/>
                <ResourceDictionary Source="/Resources/DefaultBrushes.xaml"/>
                <ResourceDictionary Source="/Resources/DataTemplates.xaml"/>
                <ResourceDictionary Source="/Resources/ButtonStyle.xaml" />
                <ResourceDictionary Source="/Resources/ColorPickerStyle.xaml" />
                <ResourceDictionary Source="/Resources/ComboBoxStyle.xaml"/>
                <ResourceDictionary Source="/Resources/DropDownStyle.xaml"/>
                <ResourceDictionary Source="/Resources/LabelStyle.xaml" />
                <ResourceDictionary Source="/Resources/TextBoxStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
